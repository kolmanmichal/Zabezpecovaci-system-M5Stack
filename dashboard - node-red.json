        "name": "Stav alarmu",
        "func": "if(msg.payload == \"AlarmAktivni\"){\n    msg.payload = '<font color=\"#ff0000\"><i class=\"fa fa-lock\"></i></font>';\n    flow.set(\"stav\",msg.payload)\n    return msg;\n}\nelse if(msg.payload == \"AlarmNeaktivni\"){\n    msg.payload = '<font color=\"#00cc00\"><i class=\"fa fa-unlock\"></i></font>';\n    flow.set(\"stav\",msg.payload)\n    return msg;\n}",
