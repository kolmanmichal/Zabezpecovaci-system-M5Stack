        "id": "0badf2e4967e4d74",
        "type": "function",
        "z": "70ef421ef9557c1e",
        "name": "Nastaveni dashboardu",
        "func": "if(msg.payload==\"AlarmAktivni\"){\n    msg.payload = {\n        \"group\": {\n            \"hide\": [\n                \"Alarm_M5Stack_System\"\n                ], \n                \"show\": [\n                    \"Alarm_M5Stack_Klavesnice\"\n                    ]\n        }\n    }\nreturn msg;\n}\n\nelse if(msg.payload==\"AlarmNeaktivni\"){\n    msg.payload = {\n        \"group\": {\n            \"show\": [\n                \"Alarm_M5Stack_System\"\n                ], \n                \"hide\": [\n                    \"Alarm_M5Stack_Klavesnice\"\n                    ]\n        }\n    }\nreturn msg;\n}\n\nelse if(msg.payload==\"CLEAR\"){\n    msg.payload = {\n        \"group\": {\n            \"show\": [\n                \"Alarm_M5Stack_System\"\n                ], \n                \"hide\": [\n                    \"Alarm_M5Stack_Klavesnice\"\n                    ]\n        }\n    }\nreturn msg;\n}",
