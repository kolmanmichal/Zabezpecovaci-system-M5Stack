        "type": "function",
        "z": "70ef421ef9557c1e",
        "name": "Stav poplachu",
        "func": "if(msg.payload == \"PoplachAktivni\"){\n    msg.payload = '<font color=\"#ff6600\"><i class=\"fa fa-exclamation-triangle\" aria-hidden=\"true\"></i></font>';\n    msg.mail = \"PoplachAktivni\"\n    flow.set(\"poplach\",msg.payload)\n    return msg;\n}\nelse if(msg.payload == \"PoplachNeaktivni\"){\n    msg.payload = \"\";\n    msg.mail = \"PoplachNeaktivni\"\n    flow.set(\"poplach\",msg.payload)\n    return msg;\n}",
